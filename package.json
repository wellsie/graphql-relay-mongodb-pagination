    "graphql": "^14.5.8",
    "graphql-redis-subscriptions": "^2.1.1",
    "graphql-subscriptions": "^1.1.0",
    "mkdirp": "^0.5.1",
    "mongodb": "^3.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
